import java.util.*;
public class JavaStudy {
static Scanner input = new Scanner (System.in);
   public static void main(String[] args)  {
      int n = input.nextInt() ; 
      int k = input.nextInt() ;
      
      String s = "" ; 
      for (int i = 0 ; i < n ; i++){
          s += (char)((int)'a'+i%k);
      }
       System.out.println(s);
      
   }
  

}
