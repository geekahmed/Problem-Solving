import java.util.*;

public class JavaStudy {
    static Scanner input = new Scanner(System.in);

    public static void main(String[] args) {
      int n = input.nextInt();
      String str = " " ;
      for (int i = 1 ; i <= 1000 ; i++){
          str += i ;

      }

      System.out.println(str.charAt(n));
       
    }



}
   
