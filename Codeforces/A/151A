import java.util.*;

public class JavaStudy {
        static Scanner input = new Scanner(System.in) ;
    public static void main(String[] args){
        int n = input.nextInt() ;
        int k = input.nextInt() ;
        int l = input.nextInt() ;
        int c = input.nextInt() ;
        int d = input.nextInt() ;
        int p = input.nextInt() ;
        int nl = input.nextInt() ;
        int np = input.nextInt()  ;


        int gas = (k*l)/(nl) ;
        int laim = (c*d) ;
        int sol = p/(np) ;

       System.out.println(Math.min(Math.min(gas,laim) , sol) / n);

    }

    }
