import java.util.*;
public class JavaStudy {

   static  Scanner input = new Scanner (System.in);
   public static void main(String[] args) {
       String s1 = input.nextLine();
       String s2 = input.nextLine();
       
       String s1_modified = s1 . toLowerCase();
       String s2_modified = s2.toLowerCase();
       
       if (s1_modified.compareTo(s2_modified) > 0 || s2_modified.compareTo(s1_modified) < 0)
       {
           System.out.println("1");
       }
       else if (s1_modified.compareTo(s2_modified) < 0 || s2_modified.compareTo(s1_modified) > 0)
       {
           System.out.println("-1");
       }
       else 
           System.out.println("0");

   }

}
